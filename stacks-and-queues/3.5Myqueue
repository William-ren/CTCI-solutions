#include <iostream>
#include <stack>
using namespace std;
class Myqueue{
private:
    stack<int> first, second;
    int topNum;
public:
    Myqueue(){ topNum=INT_MIN;};
    void push(int value){
        first.push(value);
    }
    void shiftStacks(){
        if (second.empty()) {
            while (!first.empty()) {
                second.push(first.top());
                first.pop();
            }
        }
    }
    int top(){
        shiftStacks();
        if (second.empty()) {
            return INT_MIN;
        }
        else return second.top();
    }
    void pop(){
        shiftStacks();
        if (second.empty()) {
            return;
        }
        else {
            second.pop();
        }
    }

};
