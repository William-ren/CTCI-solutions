#include <iostream>
#include <stack>
using namespace std;
stack<int> sort_stack(stack<int> & orignal){
    if (orignal.size()==0) {
        return orignal;
    }
    stack<int> buffer;
    while (!orignal.empty()) {
        int temp=orignal.top();
        orignal.pop();
        while (!buffer.empty()&&buffer.top()>temp) {
            orignal.push(buffer.top());
            buffer.pop();
        }
        buffer.push(temp);
    }
    return buffer;
}
