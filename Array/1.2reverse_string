#include <iostream>
using namespace std;
void reverse_string(char * str){
    if (str==NULL) {
        return ;
    }
    char* end=str;
    while (*end) {
        end++;
    }
    --end;
    while (str<end) {
        char tmp=*end;
        *end--=*str;
        *str++=tmp;
    }
}
int main(int argc, const char * argv[]) {
    // insert code here...
    std::cout << "Hello, World!\n";
    return 0;
}
