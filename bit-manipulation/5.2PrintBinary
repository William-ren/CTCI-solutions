#include <iostream>
#include <string>
using namespace std;
string printBinanr(double num){
    if (num>=1||num<=0) {
        return "ERROR";
    }
    double m=num;
    string result="0.";
    while (m>0) {
        if (result.size()>32) {
            return "ERROR";
        }
        int r=m*2;
        if (r>=1) {
            result+='1';
            m=r-1;
        }
        else {
            result+='0';
            m=r;
        }
    }
    return result;
}
