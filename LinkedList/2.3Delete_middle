#include <iostream>
struct ListNode{
    int val;
    ListNode * next;
};
void delete_middle(ListNode * middle){
    if (middle==NULL||middle->next==NULL) {
        return;
    }
    middle->val=middle->next->val;
    middle->next=middle->next->next;
}
int main(int argc, const char * argv[]) {
    // insert code here...
    std::cout << "Hello, World!\n";
    return 0;
}
