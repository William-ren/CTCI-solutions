#include <iostream>
struct ListNode{
    int val;
    ListNode * next;
};
ListNode * kth_element(ListNode * root,int k){
    if (root==NULL||k<0) return NULL;
    ListNode * fast=root, *slow=root;
    int counter=0;
    while (counter!=k&&fast!=NULL) {
        fast=fast->next;
    }
    if (counter!=k) return NULL;
    while (fast!=NULL) {
        fast=fast->next;
        slow=slow->next;
    }
    return slow;
}
