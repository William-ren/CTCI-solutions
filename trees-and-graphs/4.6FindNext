#include <iostream>
using namespace std;
struct TreeNode{
    int val;
    TreeNode * parent;
    TreeNode * left;
    TreeNode * right;
    TreeNode(int n): val(n),left(NULL),right(NULL){}
};
TreeNode* leftMost(TreeNode* root){
    if (root==NULL) {
        return NULL;
    }
    TreeNode* temp=root;
    while (temp->left!=NULL) {
        temp=temp->left;
    }
    return temp;
}
TreeNode * findNext(TreeNode * node) {
    if(node==NULL) return NULL;
    if (node->right!=NULL) {
        return leftMost(node->right);
    }
    TreeNode * temp=node->parent;
    TreeNode * cur=node;
    while (temp!=NULL&&temp->left!=cur) {
        cur=temp;
        temp=temp->parent;
    }
    return temp;
}
